<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DiscordBotLog</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/custom/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/custom/css/fontawesome.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/custom/css/templatemo-style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DiscordBotLog.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="tm-footer row tm-mt-small">
        <div class="col-12 font-weight-light">
            <p class="text-center text-white mb-0 px-4 small">
               @* Copyright &copy; <b>2018</b> All rights reserved.

                Design: <a rel="nofollow noopener" href="https://templatemo.com" class="tm-footer-link">Template Mo</a>*@
            </p>
        </div>
    </footer>

    <script src="~/custom/js/bootstrap.min.js"></script>
    <script src="~/custom/js/Chart.min.js"></script>
    <script src="~/custom/js/jquery-3.3.1.min.js"></script>
    <script src="~/custom/js/moment.min.js"></script>
    <script src="~/custom/js/tooplate-scripts.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
